&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt; &lt;span style="color:#a6e22e"&gt;wordIndex&lt;/span&gt; &lt;span style="color:#f92672"&gt;=&lt;/span&gt; &lt;span style="color:#ae81ff"&gt;0&lt;/span&gt;;
&lt;/span&gt;&lt;/span&gt;&lt;span style="display:flex;"&gt;&lt;span&gt; &lt;span style="color:#a6e22e"&gt;wikiButton&lt;/span&gt;.&lt;span style="color:#a6e22e"&gt;removeAttribute&lt;/span&gt;(&lt;span style="color:#e6db74"&gt;&amp;#39;disabled&amp;#39;&lt;/span&gt;);
